<p id="notice"><%= notice %></p>

<h1>Group Specifications</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_specifications.each do |group_specification| %>
      <tr>
        <td><%= group_specification.Name %></td>
        <td><%= group_specification.Description %></td>
        <td><%= link_to 'Show', group_specification %></td>
        <td><%= link_to 'Edit', edit_group_specification_path(group_specification) %></td>
        <td><%= link_to 'Destroy', group_specification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group Specification', new_group_specification_path %>
